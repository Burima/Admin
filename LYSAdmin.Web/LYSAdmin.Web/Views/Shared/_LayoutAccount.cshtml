@model LYSAdmin.Model.LoginViewModel


@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>LockYourStay Admin | @ViewBag.Title</title>

    <link href='@Url.Content("~/Content/bootstrap.min.css")' rel="stylesheet">
    <link href='@Url.Content("~/Fonts/font-awesome/css/font-awesome.css")' rel="stylesheet">

    <link href='@Url.Content("~/Content/animate.css")' rel="stylesheet">
    <link href='@Url.Content("~/Content/style.css")' rel="stylesheet">
    <!--Lockyourstay custom-->
    <link href='@Url.Content("~/Content/lockyourstay.css")' rel="stylesheet" />
    
</head>

<body class="gray-bg">
    @RenderBody()

    <!--jquery-->
    @Scripts.Render("~/bundles/jquery")
    <!--jquery validation-->
    @Scripts.Render("~/plugins/validate")
    <!-- Handler for local scripts -->
    @RenderSection("scripts", required: false)
</body>

</html>


